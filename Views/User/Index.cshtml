@model IEnumerable<FigureShop.Models.ApplicationUser>

<h2>Users</h2>
<p>
    <a href="@Url.Action("Create")">Create New User</a>
</p>
<table>
    <thead>
        <tr>
            <th>UserName</th>
            <th>Email</th>
            <th>Full Name</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.UserName</td>
                <td>@item.Email</td>
                <td>@item.FullName</td>
                <td>
                    <a href="@Url.Action("Edit", new { id = item.Id })">Edit</a> |
                    <a href="@Url.Action("Delete", new { id = item.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
